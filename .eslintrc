{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "import"],
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/forbid-prop-types": [1, {"forbid": [""]}],
    "react/jsx-props-no-spreading": [
      1,
      {
        "custom": "ignore"
      }
    ]
  },
  "env": {
    "browser": true
  }
}
